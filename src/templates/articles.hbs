{{#each this}}
<li>
    <a href="{{url}}" target="_blank" rel="noopener noreferrer">
        <article>
            <img src="{{image}}" alt="" width="480">
            <h2>{{title}}</h2>
            <p>Posted by: {{source.name}}</p>
            <p>{{description}}</p>
        </article>
    </a>
</li>
{{/each}}